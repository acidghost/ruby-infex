.row
  .col-md-5
    %b Original
  .col-md-5
    %b Tagged
  .col-md-2
    %b{:colspan => "3"}
.row.document
  .col-md-5
    %iframe{ :id => 'original-' + @document.id.to_s, :src => original_document_path }
  .col-md-5
    %iframe{ :id => 'tagged-' + @document.id.to_s, :src => tagged_document_path }
  .col-md-2
    = link_to 'Back', documents_path